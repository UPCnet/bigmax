<!doctype html>
<html metal:use-macro="api.masterTemplate.macros['master']">
<head></head>
<body>
  <metal:main fill-slot="main">
    <div class="row">
        <div class="span10 offset1">
        <table class="table table-striped">
              <thead>
                <tr>
                  <th>Date</th>
                  <th>hash</th>
                </tr>
              </thead>
              <tbody>
                <tr class="exception" tal:repeat="exception exceptions">
                  <td tal:content="exception.date"></td>
                  <td><a href="${api.context_url}/exceptions/${exception.id}" tal:content="exception.id"></a></td>
                </tr>
              </tbody>
        </table>
        </div>
    </div>

  </metal:main>
</body>
</html>
